 @model List<FirstApp.Src.Entity.ClassInfo>
  <button type="button">Create</button>
  <div class="container">
    <table class="table">
      <thead>
        <tr>
          <th>Id</th>
          <th>Name</th>
          <th>Description</th>
          <th>Created At</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
        @foreach (var item in Model)
        {
          <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Description</td>
           <td>@item.CreatedAt</td>
          </tr>
        }
      </tbody>

    </table>
  </div>

